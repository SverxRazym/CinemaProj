window.onload = function () {
    for (let i = 0; i < 19; i++) {
        let temp = "lico" + i ;
        let tempElem = document.createElement('li');
        tempElem.className = "flex-item";
        tempElem.innerHTML = " <p>SomeRoom</p>\n" +
            "            <a  id=\""+temp +"\"></a>";

        document.getElementById("Content1").append(tempElem);
    }
};


